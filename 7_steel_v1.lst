 398      uRoll.l,
 399      CO2_captured.l, CO2_total_net.l, qCCS.l, eCCS_el.l, qCCS_th.l,
 400      CostByT, TotalCost_t, TotalCost_check, ETS_cost,
 401      FuelUse_t_f;
 402   
 403  Display ProdTot, ETS_cost, CO2_intensity;
 404  Display FuelUse_t_f;


