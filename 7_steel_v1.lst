 185   
 186  * =========================
 187  * Constraints
 188  * =========================
 189   
 190  * Demand: total steel production must meet demand
 191  dem_steel(t).. sum(s, pSteel(s,t)) =g= D_steel(t);
 192   
 193  * EAF electricity use
 194  eaf_el_def(s,t)..
 195      eEAF(s,t) =e= eta_EAF(s,t) * pSteel(s,t);
 196   
 197  * Rolling mill heat requirement (assume all steel is rolled)
 198  roll_heat_def(t)..
 199      HT_roll(t) =e= TCF_roll(t) * sum(s, pSteel(s,t));
 200   
 201  * Rolling mill heat from fuels
 202  roll_fuel_def(t)..
 203      HT_roll(t) =e= sum(fuel_all, phi_roll(t,fuel_all) * uRoll(t,fuel_all));
 204   
 205  * Capacity constraint for EAF units
 206  capacity_eaf(s,t)..  pSteel(s,t) =l= Q_EAF(s,t);
 207   
 208  * =========================
 209  * CO2 & CCS equations
 210  * =========================
 211   
 212  * Scrap-related CO2 (per tonne steel)
 213  CO2_scrap_def(t)..
 214      CO2_scrap(t) =e= EF_scrap * sum(s, pSteel(s,t));
 215   
 216  * Combustion CO2 from rolling mill fuels
 217  CO2_combust_def(t)..
 218      CO2_combust(t) =e=
 219          sum(fuel_all, EF_fuel_fossil(fuel_all) * uRoll(t,fuel_all))
 220        + sum(fuel_bio, EF_bio(fuel_bio) * uRoll(t,fuel_bio));
 221   
 222  * Total CO2 available to CCS
 223  co2_offgas_pot_def(t)..
 224      CO2_offgas_pot(t) =e= CO2_scrap(t) + CO2_combust(t);
 225   
 226  * Stream to CCS cannot exceed available CO2
 227  ccs_stream_upper(t)..
 228      fCCS(t) =l= CO2_offgas_pot(t);
 229   
 230  * Lifetime-limited CCS capacity
 231  ccs_capacity_upper(t)..
 232      fCCS(t) =l=
 233         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_CCS ),
 234             qCCS(ty));
 235   
 236  * Capture efficiency
 237  ccs_capture_eff(t)..
 238      CO2_captured(t) =l= omega_ccs * fCCS(t);
 239   
 240  * Residual CO2 to atmosphere
 241  co2_atm_def(t)..
 242      CO2_atm(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 243   
 244  * Net CO2 = residual after CCS
 245  co2_total_net_def(t)..
 246      CO2_total_net(t) =e= CO2_atm(t);
 247   
 248  * CCS energy use
 249  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 250  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 251   
 252  * Investments only allowed in specific years y
 253  invest_only_y(t)$(not y(t))..
 254      qCCS(t) =e= 0;
 255   
 256  * =========================
 257  * Biogenic CO2 & ETS accounting
 258  * =========================
 259   
 260  * Total biogenic CO2 that would be emitted if there were no CCS
 261  co2_bio_pre_def(t)..
 262      CO2_bio_pre(t) =e=
 263          sum(fuel_bio, EF_bio(fuel_bio) * uRoll(t,fuel_bio));
 264   
 265  * ETS-accounted emissions:
 266  *   physical net emissions (CO2_total_net)
 267  *   minus all biogenic CO2 (treated as climate-neutral),
 268  *   allowing negative emissions when CCS > fossil emissions.
 269  co2_ets_def(t)..
 270      CO2_ets(t) =e= CO2_total_net(t) - CO2_bio_pre(t);
 271   
 272  biogas_availability(t)..  uRoll(t,'biogas') =l= biogas_availability_limit(t);
 273   
 274   
 275  * =========================
 276  * Objective function
 277  * =========================
 278  obj..
 279      TotalCost =e=
 280        sum(t,
 281             FIXEDCOST(t)
 282           + C_elec(t)       * ( sum(s, eEAF(s,t)) + eRoll(t) + eCCS_el(t) )
 283           + C_ets(t)        * CO2_ets(t)
 284           + sum(fuel_all, C_fuel(fuel_all,t) * uRoll(t,fuel_all))
 285           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 286           + C_CCS_steam(t)  * qCCS_th(t)
 287          )
 288        + sum(t$y(t), C_CCS_capex(t) * qCCS(t));
 289   
 290  * =========================
 291  * Model & Test data
 292  * =========================
 293  Model Steel7_Opt / all /;
 294   
 295  * --- Test data (example numbers, adjust as needed) ---
 296   
 297  * EAF electricity intensity (kWh/t steel)
 298  * typical EAF order of magnitude
 299  eta_EAF(s,t) = 450;
 300   
 301  * Rolling mill heat intensity (kWh/t steel)
 302  * example value
 303  TCF_roll(t)  = 600;
 304   
 305  biogas_availability_limit(t) = 10000;
 306   
 307  * EAF capacity
 308  Q_EAF(s,t) = 120000;
 309   
 310  * Costs
 311  C_elec(t)    = 0.010;
 312  C_ets(t)        = p0 + 17*(ord(t)-1);
 313  FIXEDCOST(t) = 10000;
 314   
 315  C_fuel(fuel_all,t) = 0;
 316  C_fuel('COgas',t)  = 500;
 317  C_fuel('natgas',t) = 150;
 318  C_fuel('biogas',t) = 130;
 319   
 320  * Rolling mill fuel -> heat (kWh/t fuel)
 321  Scalar
 322      LHV_COgas    / 2400 /
 323      LHV_natgas   / 13900 /
 324      LHV_biogas   / 6000 /;
 325   
 326  phi_roll(t,'COgas')  = LHV_COgas;
 327  phi_roll(t,'natgas') = LHV_natgas;
 328  phi_roll(t,'biogas') = LHV_biogas;
 329   
 330  * =========================
 331  * Solve
 332  * =========================
 333  Solve Steel7_Opt using LP minimizing TotalCost;
 334   
 335  * ===== Per-period cost breakdown =====
 336  Set costcat / fixed, elec, fuel_roll,
 337                ccs_var_ts, ccs_steam, ets,
 338                capex_ccs, total /;
 339   
 340  Parameter
 341      CostByT(t,costcat) "Cost breakdown by period (EUR)"
 342      TotalCost_t(t)     "Total cost per period (EUR)"
 343      TotalCost_check    "Sum over periods (EUR)";
 344   
 345  CostByT(t,'fixed')     = FIXEDCOST(t);
 346   
 347  CostByT(t,'elec') = C_elec(t) * ( sum(s, eEAF.l(s,t)) + eRoll.l(t) + eCCS_el.l(t) );
 348   
 349  CostByT(t,'fuel_roll')  = sum(fuel_all, C_fuel(fuel_all,t) * uRoll.l(t,fuel_all));
 350   
 351  CostByT(t,'ccs_var_ts') = ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured.l(t);
 352  CostByT(t,'ccs_steam')  = C_CCS_steam(t) * qCCS_th.l(t);
 353   
 354  CostByT(t,'ets') = C_ets(t) * CO2_ets.l(t);
 355   
 356  CostByT(t,'capex_ccs')     = 0;
 357  CostByT(t,'capex_ccs')$y(t) = C_CCS_capex(t) * qCCS.l(t);
 358   
 359  CostByT(t,'total') =  CostByT(t,'fixed')
 360                      + CostByT(t,'elec')
 361                      + CostByT(t,'fuel_roll')
 362                      + CostByT(t,'ccs_var_ts')
 363                      + CostByT(t,'ccs_steam')
 364                      + CostByT(t,'ets')
 365                      + CostByT(t,'capex_ccs');
 366   
 367  TotalCost_t(t)  = CostByT(t,'total');
 368  TotalCost_check = sum(t, TotalCost_t(t));
 369   
 370  * Fuel use per period & fuel (t)
 371  Parameter FuelUse_t_f(t,fuel_all) "Fuel use per period and fuel (t)";
 372  FuelUse_t_f(t,fuel_all) = uRoll.l(t,fuel_all);
 373   
 374   
 375   
 376  option decimals=6;
 377   
 378  Parameter CO2_total(t) "Total CO2 per period (t, net)";
 379  CO2_total(t) = CO2_total_net.l(t);
 380  Scalar CO2_total_all "Total CO2 over all periods (t, net)";
 381  CO2_total_all = sum(t, CO2_total(t));
 382  Display CO2_total, CO2_total_all;
 383   
 384  Parameter
 385      ProdTot(t)         "Total steel (t)"
 386      ETS_cost(t)        "ETS cost per period (eur)"
 387      CO2_intensity(t)   "CO2 intensity vs total product (t CO2/t steel)";
 388   
 389  ProdTot(t)      = sum(s, pSteel.l(s,t));
 390  ETS_cost(t)     = C_ets(t) * CO2_ets.l(t);
 391   
 392  Scalar ep /1e-9/;
 393  CO2_intensity(t) = CO2_total(t) / max(ProdTot(t), ep);
 394   
 395  execute_unload 'output_7Steel.gdx'
 396      CO2_scrap.l, CO2_combust.l, CO2_offgas_pot.l, CO2_atm.l,
