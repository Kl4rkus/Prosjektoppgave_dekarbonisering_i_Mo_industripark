 188  *-----------------------------*
 189  * Constraints                 *
 190  *-----------------------------*
 191   
 192  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 193  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 194   
 195  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 196   
 197  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 198  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 199  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 200   
 201  sinter_use_balance(t)..
 202      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 203   
 204  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 205  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 206   
 207  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 208  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 209   
 210  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 211   
 212  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 213  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 214   
 215  lt_saf_femn_def(s,t)..
 216      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 217  lt_saf_femn_min(s,t,fuel_lt)..
 218      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 219   
 220  ht_saf_femn_def(s,t)..
 221      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 222  ht_saf_femn_min(s,t,fuel_ht)..
 223      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 224   
 225  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 226  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 227   
 228  SiMn_material_balance(t)..
 229      sum(s, uSinterSiMn(s,t)) + pSlagTot(t) =e= sum(s, pSiMn(s,t));
 230   
 231  lt_saf_simn_def(s,t)..
 232      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 233  ht_saf_simn_def(s,t)..
 234      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 235   
 236  SiMn_energy_total(s,t)..
 237      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t) =e= TCF_SiMn(s,t) * pSiMn(s,t);
 238  SiMn_ht_min(s,t).. A_SAF_SiMn_HT_req(s) * pSiMn(s,t) =l= HT_SAF_SiMn(s,t);
 239   
 240  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t) + nu_SAF_SiMn(s,t) * pSiMn(s,t);
 241   
 242  offgas_def(s,t).. gOffgas(s,t) =e= sum(fuel_all, Y_offgas(s,t,fuel_all) * uSAF(s,t,fuel_all));
 243   
 244  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 245  CO2_offgas_def(t)..    CO2_offgas(t)    =e= (sum(s, gOffgas(s,t)) - D_offgas(t)) * EF_flare;
 246   
 247  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 248   
 249  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 250  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 251   
 252  obj..
 253      TotalCost =e= sum(t,
 254                   FIXEDCOST(t)
 255                 + C_ore(t)        * uOre(t)
 256                 + C_buysinter(t)  * bSinter(t)
 257                 + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) )
 258                 + C_ets(t)        * ( CO2_coke_sint(t) + CO2_offgas(t) )
 259                 + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 260                 + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 261                 + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 262                 );
