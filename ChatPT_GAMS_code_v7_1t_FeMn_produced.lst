 316  * =========================
 317  * Constraints
 318  * =========================
 319   
 320  dem_FeMn(t).. sum(s, pFeMn(s,t)) =e= D_FeMn(t);
 321  dem_SiMn(t).. sum(s, pSiMn(s,t)) =e= D_SiMn(t);
 322   
 323  * Split total SiMn into Si and SiO2 routes
 324  pSiMn_split_def(s,t).. pSiMn(s,t) =e= pSiMn_Si(s,t) + pSiMn_SiO2(s,t);
 325  uSinterSiMn_split_def(s,t)..
 326      uSinterSiMn(s,t) =e= uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t);
 327   
 328  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 329   
 330  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 331  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 332  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 333   
 334  sinter_use_balance(t)..
 335      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 336   
 337  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 338  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 339   
 340  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 341  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 342   
 343  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 344   
 345  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 346  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 347   
 348  lt_saf_femn_def(s,t)..
 349      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 350  lt_saf_femn_min(s,t,fuel_lt)..
 351      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 352   
 353  ht_saf_femn_def(s,t)..
 354      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 355  ht_saf_femn_min(s,t,fuel_ht)..
 356      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 357   
 358  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 359  slag_tot_def(t).. pSlagTot(t) =l= sum(s, pSlag(s,t));
 360   
 361  * SiMn material balance including slag & sinter
 362  SiMn_material_balance(t)..
 363      Y_sinter_SiMn * sum(s, uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t))
 364    + Y_slag_SiMn   * pSlagTot(t)
 365      =e= sum(s, pSiMn_Si(s,t) + pSiMn_SiO2(s,t));
 366   
 367  * Quartz and Si balances
 368  quartz_balance(s,t).. bQuartz(s,t) =e= Y_SiMn_SiO2_quartz * pSiMn_SiO2(s,t);
 369  si_balance(s,t)..     bSi(s,t)     =e= Y_SiMn_Si_silicon  * pSiMn_Si(s,t);
 370   
 371  lt_saf_simn_def(s,t)..
 372      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 373  ht_saf_simn_def(s,t)..
 374      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 375   
 376  * Route-specific SiMn heat
 377  SiMn_energy_total(s,t)..
 378      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t)
 379      =e= TCF_SiMn_Si(s,t)   * pSiMn_Si(s,t)
 380         + TCF_SiMn_SiO2(s,t)* pSiMn_SiO2(s,t);
 381   
 382  SiMn_ht_min(s,t)..
 383      A_SAF_SiMn_HT_req(s) * (pSiMn_Si(s,t) + pSiMn_SiO2(s,t)) =l= HT_SAF_SiMn(s,t);
 384   
 385  * Route-specific electricity
 386  e_saf_def(s,t)..
 387      eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t)
 388                + nu_SAF_Si(s,t)      * pSiMn_Si(s,t)
 389                + nu_SAF_SiO2(s,t)    * pSiMn_SiO2(s,t);
 390   
 391  * Off-gas production (FeMn + SiMn paths)
 392  offgas_def(s,t)..
 393      gOffgas(s,t) =e=
 394          sum(fuel_all,
 395                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 396              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 397          );
 398   
 399  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 400   
 401  CO2_offgas_def(t)..
 402      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 403   
 404  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 405   
 406  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 407  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 408   
 409  * Biocarbon capacity accumulation and usage limit
 410  biocap_def(t)..
 411      biocap(t) =e=
 412         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_Biocarbon ),
 413             qBiocarbon(ty));
 414   
 415  biocarbon_use_limit(t)..
 416      sum(s, uSAF(s,t,'biochar') + uSAF_SiMn(s,t,'biochar'))
 417    + uSint_HT(t,'biochar')
 418      =l= biocap(t);
 419   
 420  * =========================
 421  * CCS equations
 422  * =========================
 423   
 424  co2_offgas_pot_def(t)..
 425      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 426   
 427  ccs_stream_upper(t)..      fCCS(t) =l= CO2_offgas_pot(t);
 428   
 429  * Lifetime-limited CCS capacity
 430  ccs_capacity_upper(t)..
 431      fCCS(t) =l=
 432         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_CCS ),
 433             qCCS(ty));
 434   
 435  ccs_capture_eff(t)..
 436      CO2_captured(t) =l= omega_ccs * fCCS(t);
 437   
 438  co2_atm_offgas_def(t)..
 439      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 440   
 441  co2_total_net_def(t)..
 442      CO2_total_net(t) =e= CO2_coke_sint(t) + CO2_atm_offgas(t);
 443   
