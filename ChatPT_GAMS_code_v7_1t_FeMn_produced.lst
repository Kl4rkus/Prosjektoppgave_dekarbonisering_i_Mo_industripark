 138  Parameter EF_bio(fuel_all) "t CO2 per t fuel (nonzero only for biogenic)";
 139  EF_bio(fuel_all) = 0;
 140  EF_bio('biochar')   = 3.0;
 141  EF_bio('woodchips') = 1.8;
 142  EF_bio('biooil')    = 3.0;
 143  EF_bio('biogas')    = 2.0;
 144   
 145  * Fossil and total (fossil+biogenic) CO2 emission factors per fuel
 146  * Units: t CO2 per t fuel
 147  Parameter
 148      EF_fossil(fuel_all) "Fossil CO2 per t fuel"
 149      EF_tot(fuel_all)    "Total (fossil+biogenic) CO2 per t fuel";
 150   
 151  * Default: zero
 152  EF_fossil(fuel_all) = 0;
 153   
 154  * Approximate fossil CO2 emission factors (based on typical carbon content & LHVs)
 155  * high-carbon solid fuel
 156  EF_fossil('coke')     = 3.3;
 157  * heavy fuel oil
 158  EF_fossil('oil')      = 3.1;
 159  * natural gas
 160  EF_fossil('natgas')   = 2.7;
 161  * CO-rich off-gas, low LHV
 162  EF_fossil('COgas')    = 0.4;
 163  * no direct CO2 on combustion
 164  EF_fossil('hydrogen') = 0.0;
 165   
 166  * Biofuels: fossil part = 0; their CO2 is fully in EF_bio
 167  EF_fossil('biochar')   = 0;
 168  EF_fossil('woodchips') = 0;
 169  EF_fossil('biooil')    = 0;
 170  EF_fossil('biogas')    = 0;
 171   
 172  * Total CO2 emission factor per fuel = fossil + biogenic
 173  EF_tot(fuel_all) = EF_fossil(fuel_all) + EF_bio(fuel_all);
