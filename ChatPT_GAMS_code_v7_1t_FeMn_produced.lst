 252  Equation
 253      dem_FeMn(t)
 254      dem_SiMn(t)
 255   
 256      ore_to_sinter(t)
 257      sinter_energy_total(t)
 258      sinter_ht_min(t)
 259      sinter_power(t)
 260      sinter_use_balance(t)
 261      lt_sint_def(t)
 262      lt_sint_min_lt(fuel_lt,t)
 263      ht_sint_def(t)
 264      ht_sint_min_ht(fuel_ht,t)
 265   
 266      FeMn_sinter_link(s,t)
 267      FeMn_energy_total(s,t)
 268      FeMn_ht_min(s,t)
 269      lt_saf_femn_def(s,t)
 270      lt_saf_femn_min(s,t,fuel_lt)
 271      ht_saf_femn_def(s,t)
 272      ht_saf_femn_min(s,t,fuel_ht)
 273   
 274      slag_def(s,t)
 275      slag_tot_def(t)
 276      SiMn_material_balance(t)
 277      pSiMn_split_def(s,t)
 278      uSinterSiMn_split_def(s,t)
 279      quartz_balance(s,t)
 280      si_balance(s,t)
 281   
 282      lt_saf_simn_def(s,t)
 283      ht_saf_simn_def(s,t)
 284      SiMn_energy_total(s,t)
 285      SiMn_ht_min(s,t)
 286   
 287      e_saf_def(s,t)
 288   
 289      offgas_def(s,t)
 290      CO2_coke_sint_def(t)
 291      CO2_offgas_def(t)
 292      coke_total_def(t)
 293   
 294      capacity_saf(s,t)
 295      capacity_sinter(t)
 296   
 297      biocap_def(t)
 298      biocarbon_use_limit(t)
 299   
 300      co2_offgas_pot_def(t)
 301      ccs_stream_upper(t)
 302      ccs_capacity_upper(t)
 303      ccs_capture_eff(t)
 304      co2_atm_offgas_def(t)
 305      co2_total_net_def(t)
 306      ccs_el_def(t)
 307      ccs_th_def(t)
 308      invest_only_y(t)
 309      invest_biocarbon_only_y(t)
