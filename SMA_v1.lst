 177  * =========================
 178  * Equations
 179  * =========================
 180  Equation
 181      dem_CaO(t)
 182   
 183      CaO_energy_total(s,t)
 184      CaO_ht_min(s,t)
 185      lt_saf_cao_def(s,t)
 186      lt_saf_cao_min(s,t,fuel_lt)
 187      ht_saf_cao_def(s,t)
 188      ht_saf_cao_min(s,t,fuel_ht)
 189   
 190      e_saf_def(s,t)
 191      capacity_saf(s,t)
 192   
 193      biocap_def(t)
 194      biocarbon_use_limit(t)
 195   
 196      CO2_calc_def(t)
 197      co2_offgas_pot_def(t)
 198      ccs_stream_upper(t)
 199      ccs_capacity_upper(t)
 200      ccs_capture_eff(t)
 201      co2_atm_offgas_def(t)
 202      co2_total_net_def(t)
 203      ccs_el_def(t)
 204      ccs_th_def(t)
 205      invest_only_y(t)
 206      invest_biocarbon_only_y(t)
 207   
 208      co2_bio_pre_def(t)   "Biogenic CO2 pre-capture"
 209      co2_ets_def(t)       "ETS-accounted emissions"
 210   
 211   
 212  * NEW constraints:
 213      no_fuel_elec(s,t,fuel_all)  "Electric furnace uses no fuels"
 214      LT_elec_zero(s,t)           "No LT heat balance for electric furnace"
 215      HT_elec_zero(s,t)           "No HT heat balance for electric furnace"
 216      furnace2_off(t_pre)         "Electric furnace s2 off in t1–t5"
 217   
 218      obj;
 219   
 220  * =========================
 221  * Constraints
 222  * =========================
 223   
 224  * Demand: total CaO production must meet demand
 225  dem_CaO(t).. sum(s, pCaO(s,t)) =g= D_CaO(t);
 226   
 227  * Energy balance: total LT+HT heat equals total heat requirement per t CaO
 228  * ONLY for fuel-based shaft kiln s1
 229  CaO_energy_total(s,t)$(s_fuel(s))..
 230      LT_SAF_CaO(s,t) + HT_SAF_CaO(s,t) =e= TCF_CaO(s,t) * pCaO(s,t);
 231   
 232  * Minimum HT requirement (only for s1)
 233  CaO_ht_min(s,t)$(s_fuel(s))..
 234      A_SAF_HT_req(s) * pCaO(s,t) =l= HT_SAF_CaO(s,t);
 235   
 236  * LT heat from LT fuels (only for s1)
 237  lt_saf_cao_def(s,t)$(s_fuel(s))..
 238      LT_SAF_CaO(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 239   
 240  lt_saf_cao_min(s,t,fuel_lt)$(s_fuel(s))..
 241      A_saf_lt(s,fuel_lt) * LT_SAF_CaO(s,t) =l= uSAF(s,t,fuel_lt);
 242   
 243  * HT heat from HT fuels (only for s1)
 244  ht_saf_cao_def(s,t)$(s_fuel(s))..
 245      HT_SAF_CaO(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 246   
 247  ht_saf_cao_min(s,t,fuel_ht)$(s_fuel(s))..
 248      A_saf_ht(s,fuel_ht) * HT_SAF_CaO(s,t) =l= uSAF(s,t,fuel_ht);
 249   
 250  * NEW: Electric furnace s2 – no fuel use at all
 251  no_fuel_elec(s,t,fuel_all)$(s_elec(s))..
 252      uSAF(s,t,fuel_all) =e= 0;
 253   
 254  * NEW: Electric furnace s2 – no LT/HT heat variables used
 255  LT_elec_zero(s,t)$(s_elec(s))..
 256      LT_SAF_CaO(s,t) =e= 0;
 257   
 258  HT_elec_zero(s,t)$(s_elec(s))..
 259      HT_SAF_CaO(s,t) =e= 0;
 260   
 261  * Route-specific electricity: both furnaces use electricity
 262  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_CaO(s,t) * pCaO(s,t);
 263   
 264  * Capacity constraint for SMA units
 265  capacity_saf(s,t)..  pCaO(s,t) =l= Q_SAF(s,t);
 266   
 267  * NEW: Electric furnace only operational from t6 onwards
 268  furnace2_off(t_pre)..
 269      pCaO('s2',t_pre) =e= 0;
 270   
 271  * Biocarbon capacity accumulation and usage limit
 272  biocap_def(t)..
 273      biocap(t) =e=
 274         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_Biocarbon ),
 275             qBiocarbon(ty));
 276   
 277  biocarbon_use_limit(t)..
 278      sum(s, uSAF(s,t,'biochar')) =l= biocap(t);
 279   
 280  * =========================
 281  * CO2 from calcination & CCS equations
 282  * =========================
 283   
 284  * CO2 from CaCO3 -> CaO + CO2
 285  CO2_calc_def(t)..
 286      CO2_calc(t) =e= EF_calc_CaO * sum(s, pCaO(s,t));
 287   
 288  * All capture-eligible CO2 is calcination CO2
 289  co2_offgas_pot_def(t)..
 290      CO2_offgas_pot(t) =e= CO2_calc(t);
 291   
 292  * CCS stream bounded by available CO2
 293  ccs_stream_upper(t)..
 294      fCCS(t) =l= CO2_offgas_pot(t);
 295   
 296  * Lifetime-limited CCS capacity
 297  ccs_capacity_upper(t)..
 298      fCCS(t) =l=
 299         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_CCS ),
 300             qCCS(ty));
 301   
 302  * Capture efficiency
 303  ccs_capture_eff(t)..
 304      CO2_captured(t) =l= omega_ccs * fCCS(t);
 305   
 306  * Residual CO2 to atmosphere
 307  co2_atm_offgas_def(t)..
 308      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 309   
 310  * Net CO2 = residual calcination CO2
 311  co2_total_net_def(t)..
 312      CO2_total_net(t) =e= CO2_atm_offgas(t);
 313   
 314  * CCS energy use
 315  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 316  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 317   
 318  * Investments only allowed in specific years y
 319  invest_only_y(t)$(not y(t))..
 320      qCCS(t) =e= 0;
 321   
 322  invest_biocarbon_only_y(t)$(not y(t))..
 323      qBiocarbon(t) =e= 0;
 324   
 325  * =========================
 326  * Biogenic CO2 & ETS accounting
 327  * =========================
 328   
 329  * Total biogenic CO2 that would be emitted if there were no CCS
 330  co2_bio_pre_def(t)..
 331      CO2_bio_pre(t) =e=
 332            sum(s, sum(fuel_bio,
 333                          EF_bio(fuel_bio) * uSAF(s,t,fuel_bio)));
 334   
 335  * ETS-accounted emissions:
 336  *   physical net emissions (CO2_total_net)
 337  *   minus all biogenic CO2 (treated as climate-neutral),
 338  *   allowing negative emissions when CCS > fossil emissions.
 339  co2_ets_def(t)..
 340      CO2_ets(t) =e= CO2_total_net(t) - CO2_bio_pre(t);
 341   
 342  * =========================
 343  * Objective function
 344  * =========================
 345  obj..
 346      TotalCost =e=
 347        sum(t,
 348             FIXEDCOST(t)
 349           + C_elec(t)       * ( sum(s, eSAF(s,t)) + eCCS_el(t) )
 350           + C_ets(t)        * CO2_ets(t)
 351           + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * uSAF(s,t,fuel_all)))
 352           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 353           + C_CCS_steam(t)  * qCCS_th(t)
 354          )
 355        + sum(t$y(t), C_CCS_capex(t)        * qCCS(t))
 356        + sum(t$y(t), C_Biocarbon_capex(t)  * qBiocarbon(t));
 357   
 358  * =========================
 359  * Model & Test data
 360  * =========================
 361  Model SMA_CaCO3_Opt / all /;
 362   
 363  * --- Test data (example numbers) ---
 364   
 365  * Heat & electricity intensities
 366  eta_SAF_CaO('s1',t) = 60;
 367  eta_SAF_CaO('s2',t) = 1300;
 368   
 369  * kWh/t CaO total thermal heat (only used for s1 via s_fuel)
 370  TCF_CaO(s,t)    = 1200;
 371   
 372  * can set to 1 if you want only HT
 373  A_SAF_HT_req(s) = 0.2;
 374   
 375  A_saf_lt(s,fuel_lt) = 0;
 376  A_saf_ht(s,fuel_ht) = 0;
 377   
 378  * capacity
 379  Q_SAF(s,t) = 120000;
 380   
 381  C_coke(t)       = 300;
 382  C_elec(t)       = 0.010;
 383  C_ets(t)        = p0 + 17*(ord(t)-1);
 384  FIXEDCOST(t)    = 000;
 385   
 386  C_fuel(fuel_all,t) = 0;
 387  C_fuel('coke',t)      = C_coke(t);
 388  C_fuel('oil',t)       = 500;
 389  C_fuel('biooil',t)    = 450;
 390  C_fuel('woodchips',t) = 1200;
 391  C_fuel('COgas',t)     = 50;
 392  C_fuel('hydrogen',t)  = 2000;
 393  C_fuel('natgas',t)    = 1500;
 394  C_fuel('biogas',t)    = 1300;
 395  C_fuel('biochar',t)   = 4000;
 396   
 397  phi_saf_lt(s,t,fuel_lt) = 0;
 398  phi_saf_ht(s,t,fuel_ht) = 0;
 399   
 400  Scalar
 401      LHV_coke       /  8000 /
 402      LHV_biochar    /  7000 /
 403      LHV_oil        /  1160 /
 404      LHV_biooil     /  5000 /
 405      LHV_woodchips  /  2500 /
 406      LHV_natgas     /  1390 /
 407      LHV_biogas     /  6000 /
 408      LHV_COgas      /  2400 /
 409      LHV_hydrogen   / 33300 /;
 410   
 411  * Example mapping of fuels to HT/LT heat
 412  phi_saf_ht(s,t,'coke')     = LHV_coke;
 413  phi_saf_ht(s,t,'biochar')  = LHV_biochar;
 414  phi_saf_ht(s,t,'COgas')    = LHV_COgas;
 415  phi_saf_ht(s,t,'hydrogen') = LHV_hydrogen;
 416  phi_saf_ht(s,t,'natgas')   = LHV_natgas;
 417  phi_saf_ht(s,t,'biogas')   = LHV_biogas;
 418   
 419  phi_saf_lt(s,t,'oil')       = LHV_oil;
 420  phi_saf_lt(s,t,'biooil')    = LHV_biooil;
 421  phi_saf_lt(s,t,'woodchips') = LHV_woodchips;
 422   
 423  * =========================
 424  * Solve
 425  * =========================
 426  Solve SMA_CaCO3_Opt using LP minimizing TotalCost;
 427   
 428  * ===== Per-period cost breakdown =====
 429  Set costcat / fixed, elec, fuel_SAF,
 430                ccs_var_ts, ccs_steam, ets,
 431                capex_ccs, capex_biocarb, total /;
 432   
 433  Parameter
 434      CostByT(t,costcat) "Cost breakdown by period (EUR)"
 435      TotalCost_t(t)     "Total cost per period (EUR)"
 436      TotalCost_check    "Sum over periods (EUR)";
 437   
 438  CostByT(t,'fixed')     = FIXEDCOST(t);
 439   
 440  CostByT(t,'elec') = C_elec(t) * ( sum(s, eSAF.l(s,t)) + eCCS_el.l(t) );
 441   
 442  CostByT(t,'fuel_SAF')  = sum(s, sum(fuel_all, C_fuel(fuel_all,t) * uSAF.l(s,t,fuel_all)));
 443   
 444  CostByT(t,'ccs_var_ts') = ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured.l(t);
 445  CostByT(t,'ccs_steam')  = C_CCS_steam(t) * qCCS_th.l(t);
 446   
 447  CostByT(t,'ets') = C_ets(t) * CO2_ets.l(t);
 448   
 449  CostByT(t,'capex_ccs')     = 0;
 450  CostByT(t,'capex_ccs')$y(t)     = C_CCS_capex(t)       * qCCS.l(t);
 451  CostByT(t,'capex_biocarb') = 0;
 452  CostByT(t,'capex_biocarb')$y(t) = C_Biocarbon_capex(t) * qBiocarbon.l(t);
 453   
 454  CostByT(t,'total') =  CostByT(t,'fixed')
 455                      + CostByT(t,'elec')
 456                      + CostByT(t,'fuel_SAF')
 457                      + CostByT(t,'ccs_var_ts')
 458                      + CostByT(t,'ccs_steam')
 459                      + CostByT(t,'ets')
 460                      + CostByT(t,'capex_ccs')
 461                      + CostByT(t,'capex_biocarb');
 462   
 463  TotalCost_t(t)  = CostByT(t,'total');
 464  TotalCost_check = sum(t, TotalCost_t(t));
 465   
 466  * >>> NEW: Aggregate fuel use per period and fuel
 467  Parameter FuelUse_t_f(t,fuel_all) "Fuel use per period and fuel (t)";
 468  FuelUse_t_f(t,fuel_all) = sum(s, uSAF.l(s,t,fuel_all));
 469   
 470  * ---- CO2 & ETS summary ----
 471  Parameter CO2_total(t) "Total CO2 per period (t, net)";
 472  Scalar   CO2_total_all "Total CO2 over all periods (t, net)";
 473   
 474  CO2_total(t)    = CO2_total_net.l(t);
 475  CO2_total_all   = sum(t, CO2_total(t));
 476   
 477  Parameter
 478      ProdTot(t)       "Total CaO (t)"
 479      ETS_cost(t)      "ETS cost per period (eur)"
 480      CO2_intensity(t) "CO2 intensity vs total product (t CO2/t product)";
 481   
 482  ProdTot(t)  = sum(s, pCaO.l(s,t));
 483  ETS_cost(t) = C_ets(t) * CO2_ets.l(t);
 484   
 485  Scalar ep /1e-9/;
 486  CO2_intensity(t) = CO2_total(t) / max(ProdTot(t), ep);
 487   
 488  * ---- Write to GDX ----
 489  execute_unload 'output_SMA.gdx'
 490      CO2_calc.l, CO2_offgas_pot.l, CO2_atm_offgas.l,
 491      pCaO.l, eSAF.l,
 492      CO2_captured.l, CO2_total_net.l, qCCS.l, eCCS_el.l, qCCS_th.l,
 493      qBiocarbon.l, biocap.l,
 494      CostByT, TotalCost_t, TotalCost_check, costcat,
 495      FuelUse_t_f,
 496      ETS_cost;
 497   
 498  option decimals=6;
 499   
 500  Display CO2_total, CO2_total_all;
 501  Display ProdTot, ETS_cost, CO2_intensity;
 502  Display FuelUse_t_f;
