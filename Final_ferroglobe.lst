 382   
 383  * Objective function
 384  obj..
 385      z =E= OPEX + CAPEX;
 386   
 387  * CAPEX equation
 388  capex_cost..
 389      CAPEX =E= sum(q, (1 / power(1 + R, ord(q))) * sum(f, C_Invest(f, q) * w_max(f, q)));
 390   
 391  * Cost of materials bought
 392  cost_mat..
 393      costs_mat =E= sum((m, t), g(m, t) * C_Mat(m, t));
 394   
 395   
 396  * Cost of electricity
 397  cost_elec..
 398      costs_elec =E= sum((p, t), E(p) * C_Elec(t) * w(p, t));
 399   
 400  * Carbon tax
 401  cost_carbon..
 402      costs_carbon =E= sum(t, tc(t) * C_Carbon(t));
 403   
 404  * Costs due to material overflows.
 405  overflow_costs..
 406      costs_overflow =E= sum((m,t), overflow_cost(m)*overflow(m,t));
 407   
 408  * OPEX equation
 409  opex_cost..
 410      OPEX =E= costs_mat + costs_elec + costs_carbon + costs_overflow;
 411   
 412   
 413   
 414  * Balance constraint
 415  balance(m, t)..
 416      g(m, t) + sum(p$M_output(p, m), o(m, p, t)) =E= D(m, t) + sum(p$M_input(p, m), i(m, p, t)) + overflow(m, t);
 417  * Heat constraint
 418  heat(p, t)..
 419      sum(l, u(l, p, t) * LTR(l)) + sum(h, b(h, p, t) * HTR(h)) =G= w(p, t) * TH(p);
 420  * High heat constraint
 421  high_heat(p, t)..
 422      sum(h, b(h, p, t) * HTR(h)) =G= w(p, t) * TH(p) * HT(p);
 423  * Input constraint
 424  input(m, p, t)$M_input(p, m)..
 425      i(m, p, t) =E= PRODUCE(m, p) * w(p, t);
 426  * Output constraint
 427  output(m, p, t)$(M_output(p, m) AND m_include(m))..
 428      o(m, p, t) =E= Y(m, p) * w(p, t);
 429   
 430  * Carbon emissions constraint
 431  carbon(p,t)..
 432      c(p,t) =E= sum(h, b(h, p, t) * HEF(h, p)*(1-offgas_coef(p)))
 433                + sum(l, u(l, p, t) * LEF(l, p))*(1-offgas_coef(p));
 434   
 435   
 436  offgas_out(p,t)..
 437      o('off-gas',p,t) =E=  sum(h, b(h, p, t) * HEF(h, p)*(offgas_coef(p))) + sum(l, u(l, p, t) * LEF(l, p))*(offgas_coef(p));
 438   
 439  total_carbon(t)..
 440      tc(t) =E= sum(p, c(p,t)) + w('flaring', t);
 441   
 442  * Capacity constraint
 443  capacity(f, t)..
 444      sum(p$P_f(f, p), w(p, t)) =L= sum(q$(ord(q) <= ord(t)), w_max(f, q));
