 375  * Variables for cost components
 376  VARIABLE
 377      costs_mat       'Costs of materials bought'
 378      costs_fuel      'Costs of fuels (low and high temp)'
 379      costs_elec      'Costs of electricity'
 380      costs_carbon    'Carbon tax'
 381      costs_overflow 'costs of trashing materials'
 382      OPEX            'Total operational cost'
 383      CAPEX           'Total capital cost'
 384  ;
 385   
 386  * Objective function
 387  obj..
 388      z =E= OPEX + CAPEX;
 389   
 390  * CAPEX equation
 391  capex_cost..
 392      CAPEX =E= sum(q, (1 / power(1 + R, ord(q))) * sum(f, C_Invest(f, q) * w_max(f, q)));
 393   
 394  * Cost of materials bought
 395  cost_mat..
 396      costs_mat =E= sum((m, t), g(m, t) * C_Mat(m, t));
 397   
 398   
 399  * Cost of electricity
 400  cost_elec..
 401      costs_elec =E= sum((p, t), E(p) * C_Elec(t) * w(p, t));
 402   
 403  * Carbon tax
 404  cost_carbon..
 405      costs_carbon =E= sum(t, (tc(t)-captured_carbon(t) * C_Carbon(t)));
 406   
 407  * Costs due to material overflows.
 408  overflow_costs..
 409      costs_overflow =E= sum((m,t), overflow_cost(m)*overflow(m,t));
 410   
 411  * OPEX equation
 412  opex_cost..
 413      OPEX =E= costs_mat + costs_elec + costs_carbon + costs_overflow;
 414   
 415   
 416   
 417  * Balance constraint
 418  balance(m, t)..
 419      g(m, t) + sum(p$M_output(p, m), o(m, p, t)) =E= D(m, t) + sum(p$M_input(p, m), i(m, p, t)) + overflow(m, t);
 420  * Heat constraint
 421  heat(p, t)..
 422      sum(l, u(l, p, t) * LTR(l)) + sum(h, b(h, p, t) * HTR(h)) =E= w(p, t) * TH(p);
 423  * High heat constraint
 424  high_heat(p, t)..
 425      sum(h, b(h, p, t) * HTR(h)) =G= w(p, t) * TH(p) * HT(p);
 426  * Input constraint
 427  input(m, p, t)$M_input(p, m)..
 428      i(m, p, t) =E= PRODUCE(m, p) * w(p, t);
 429  * Output constraint
 430  output(m, p, t)$(M_output(p, m) AND m_include(m))..
 431      o(m, p, t) =E= Y(m, p) * w(p, t);
 432   
 433  * Carbon emissions constraint
 434  carbon(p,t)..
 435      c(p,t) =E= sum(h, b(h, p, t) * HEF(h, p)*(1-offgas_coef(p)))
 436                + sum(l, u(l, p, t) * LEF(l, p))*(1-offgas_coef(p));
 437   
 438   
 439  offgas_out(p,t)..
 440      o('off-gas',p,t) =E=  sum(h, b(h, p, t) * HEF(h, p)*(offgas_coef(p))) + sum(l, u(l, p, t) * LEF(l, p))*(offgas_coef(p));
 441   
 442  total_carbon(t)..
 443      tc(t) =E= sum(p, c(p,t)) + w('flaring', t);
 444   
 445  * Capacity constraint
 446  capacity(f, t)..
 447      sum(p$P_f(f, p), w(p, t)) =L= sum(q$(ord(q) <= ord(t)), w_max(f, q));
 448   
 449  * Carbon capture:
 450  carbon_capture(t)..
 451      captured_carbon(t) =L= tc(t) * capture_rate;
 452   
 453  max_carbon_capture(t)..
 454      captured_carbon(t) =L= sum(q$(ord(q) <= ord(t)), w_max('CCS', q));
 455   
