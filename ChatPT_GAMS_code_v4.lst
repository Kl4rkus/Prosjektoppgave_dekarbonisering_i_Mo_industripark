 189  *-----------------------------*
 190  * Constraints                 *
 191  *-----------------------------*
 192   
 193  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 194  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 195   
 196  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 197   
 198  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 199  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 200  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 201   
 202  sinter_use_balance(t)..
 203      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 204   
 205  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 206  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 207   
 208  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 209  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 210   
 211  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 212   
 213  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 214  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 215   
 216  lt_saf_femn_def(s,t)..
 217      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 218  lt_saf_femn_min(s,t,fuel_lt)..
 219      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 220   
 221  ht_saf_femn_def(s,t)..
 222      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 223  ht_saf_femn_min(s,t,fuel_ht)..
 224      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 225   
 226  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 227  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 228   
 229  SiMn_material_balance(t)..
 230      sum(s, uSinterSiMn(s,t)) + pSlagTot(t) =e= sum(s, pSiMn(s,t));
 231   
 232  lt_saf_simn_def(s,t)..
 233      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 234  ht_saf_simn_def(s,t)..
 235      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 236   
 237  SiMn_energy_total(s,t)..
 238      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t) =e= TCF_SiMn(s,t) * pSiMn(s,t);
 239  SiMn_ht_min(s,t).. A_SAF_SiMn_HT_req(s) * pSiMn(s,t) =l= HT_SAF_SiMn(s,t);
 240   
 241  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t) + nu_SAF_SiMn(s,t) * pSiMn(s,t);
 242   
 243  * --- Off-gas production (FeMn + SiMn paths) ---
 244  offgas_def(s,t)..
 245      gOffgas(s,t) =e=
 246          sum(fuel_all,
 247                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 248              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 249          );
 250   
 251  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 252   
 253  * --- CO2 from flaring (dimension matches declaration over t) ---
 254  CO2_offgas_def(t)..
 255      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 256   
 257  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 258   
 259  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 260  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 261   
 262  obj..
 263      TotalCost =e= sum(t,
 264                   FIXEDCOST(t)
 265                 + C_ore(t)        * uOre(t)
 266                 + C_buysinter(t)  * bSinter(t)
 267                 + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) )
 268                 + C_ets(t)        * ( CO2_coke_sint(t) + CO2_offgas(t) )
 269                 + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 270                 + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 271                 + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 272                 );
