 194  * =========================
 195  * Equations
 196  * =========================
 197  Equation
 198      dem_FeSi(t)
 199   
 200      pFeSi_split_def(s,t)
 201      quartz_balance(s,t)
 202      si_balance(s,t)
 203   
 204      FeSi_energy_total(s,t)
 205      FeSi_ht_min(s,t)
 206      lt_saf_fesi_def(s,t)
 207      lt_saf_fesi_min(s,t,fuel_lt)
 208      ht_saf_fesi_def(s,t)
 209      ht_saf_fesi_min(s,t,fuel_ht)
 210   
 211      e_saf_def(s,t)
 212   
 213      offgas_def(s,t)
 214   
 215      capacity_saf(s,t)
 216   
 217      biocap_def(t)
 218      biocarbon_use_limit(t)
 219   
 220      co2_offgas_pot_def(t)
 221      ccs_stream_upper(t)
 222      ccs_capacity_upper(t)
 223      ccs_capture_eff(t)
 224      co2_atm_offgas_def(t)
 225      co2_total_net_def(t)
 226      ccs_el_def(t)
 227      ccs_th_def(t)
 228      invest_only_y(t)
 229      invest_biocarbon_only_y(t)
