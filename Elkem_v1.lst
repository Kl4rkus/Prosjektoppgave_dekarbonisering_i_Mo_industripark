 234      obj;
 235   
 236  * =========================
 237  * Constraints
 238  * =========================
 239   
 240  * Demand: total FeSi production must meet demand
 241  dem_FeSi(t).. sum(s, pFeSi(s,t)) =g= D_FeSi(t);
 242   
 243  * Split total FeSi into Si and SiO2 routes
 244  pFeSi_split_def(s,t).. pFeSi(s,t) =e= pFeSi_Si(s,t) + pFeSi_SiO2(s,t);
 245   
 246  * Quartz and Si balances
 247  quartz_balance(s,t).. bQuartz(s,t) =e= Y_FeSi_SiO2_quartz * pFeSi_SiO2(s,t);
 248  si_balance(s,t)..     bSi(s,t)     =e= Y_FeSi_Si_silicon  * pFeSi_Si(s,t);
 249   
 250  * Energy balance: total LT+HT heat equals total heat requirement
 251  FeSi_energy_total(s,t)..
 252      LT_SAF_FeSi(s,t) + HT_SAF_FeSi(s,t) =e= TCF_FeSi(s,t) * pFeSi(s,t);
 253   
 254  * Minimum HT requirement (can be set to 0 if not needed)
 255  FeSi_ht_min(s,t)..
 256      A_SAF_HT_req(s) * pFeSi(s,t) =l= HT_SAF_FeSi(s,t);
 257   
 258  * LT heat from LT fuels
 259  lt_saf_fesi_def(s,t)..
 260      LT_SAF_FeSi(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 261   
 262  lt_saf_fesi_min(s,t,fuel_lt)..
 263      A_saf_lt(s,fuel_lt) * LT_SAF_FeSi(s,t) =l= uSAF(s,t,fuel_lt);
 264   
 265  * HT heat from HT fuels
 266  ht_saf_fesi_def(s,t)..
 267      HT_SAF_FeSi(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 268   
 269  ht_saf_fesi_min(s,t,fuel_ht)..
 270      A_saf_ht(s,fuel_ht) * HT_SAF_FeSi(s,t) =l= uSAF(s,t,fuel_ht);
 271   
 272  * Electricity use
 273  e_saf_def(s,t)..
 274      eSAF(s,t) =e= eta_SAF_FeSi(s,t) * pFeSi(s,t);
 275   
 276  * Off-gas production
 277  offgas_def(s,t)..
 278      gOffgas(s,t) =e= sum(fuel_all, Y_offgas(s,t,fuel_all) * uSAF(s,t,fuel_all));
 279   
 280  * Capacity constraint for SAF units
 281  capacity_saf(s,t)..  pFeSi(s,t) =l= Q_SAF(s,t);
 282   
 283  * Biocarbon capacity accumulation and usage limit
 284  biocap_def(t)..
 285      biocap(t) =e=
 286         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_Biocarbon ),
 287             qBiocarbon(ty));
 288   
 289  biocarbon_use_limit(t)..
 290      sum(s, uSAF(s,t,'biochar')) =l= biocap(t);
 291   
 292  * =========================
 293  * CCS equations
 294  * =========================
 295   
 296  co2_offgas_pot_def(t)..
 297      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 298   
 299  ccs_stream_upper(t)..
 300      fCCS(t) =l= CO2_offgas_pot(t);
 301   
 302  * Lifetime-limited CCS capacity
 303  ccs_capacity_upper(t)..
 304      fCCS(t) =l=
 305         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_CCS ),
 306             qCCS(ty));
 307   
 308  ccs_capture_eff(t)..
 309      CO2_captured(t) =l= omega_ccs * fCCS(t);
 310   
 311  co2_atm_offgas_def(t)..
 312      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 313   
 314  co2_total_net_def(t)..
 315      CO2_total_net(t) =e= CO2_atm_offgas(t);
 316   
 317  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 318  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 319   
 320  invest_only_y(t)$(not y(t))..
 321      qCCS(t) =e= 0;
 322   
 323  invest_biocarbon_only_y(t)$(not y(t))..
 324      qBiocarbon(t) =e= 0;
 325   
 326  * =========================
 327  * Biogenic CO2 & ETS accounting
 328  * =========================
 329   
 330  * Total biogenic CO2 that would be emitted if there were no CCS
 331  co2_bio_pre_def(t)..
 332      CO2_bio_pre(t) =e=
 333            sum(s, sum(fuel_bio,
 334                          EF_bio(fuel_bio) * uSAF(s,t,fuel_bio)));
 335   
 336  * ETS-accounted emissions:
 337  *   physical net emissions (CO2_total_net)
 338  *   minus all biogenic CO2 (treated as climate-neutral),
 339  *   which allows negative emissions when CCS captures more
 340  *   than fossil emissions.
 341  co2_ets_def(t)..
 342      CO2_ets(t) =e= CO2_total_net(t) - CO2_bio_pre(t);
 343   
 344  * =========================
 345  * Objective function
 346  * =========================
 347  obj..
 348      TotalCost =e=
 349        sum(t,
 350             FIXEDCOST(t)
 351           + C_elec(t)       * ( sum(s, eSAF(s,t)) + eCCS_el(t) )
 352           + C_ets(t)        * CO2_ets(t)
 353           + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * uSAF(s,t,fuel_all)))
 354           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 355           + C_CCS_steam(t)  * qCCS_th(t)
 356           + sum(s, C_quartz(t) * bQuartz(s,t))
 357           + sum(s, C_Si(t)     * bSi(s,t))
 358          )
 359        + sum(t$y(t), C_CCS_capex(t)       * qCCS(t))
 360        + sum(t$y(t), C_Biocarbon_capex(t) * qBiocarbon(t));
 361   
 362  * =========================
 363  * Model & Test data
 364  * =========================
 365  Model Elkem_FeSi_Opt / all /;
 366   
 367  * --- Test data (example numbers; adjust to Elkem data) ---
 368   
 369  Y_FeSi_SiO2_quartz = 0.4;
 370  Y_FeSi_Si_silicon  = 0.4;
 371   
 372  Y_offgas(s,t,fuel_all) = 1.8;
 373  EF_flare              = 1.5;
 374  D_offgas(t)           = 0;
 375   
 376  * kWh/t FeSi (example)
 377  eta_SAF_FeSi(s,t) = 6000;
 378  * thermal kWh/t FeSi (example)
 379  TCF_FeSi(s,t)     = 5000;
 380   
 381  A_SAF_HT_req(s) = 0.2;
 382   
 383  A_saf_lt(s,fuel_lt) = 0;
 384  A_saf_ht(s,fuel_ht) = 0;
 385   
 386  Q_SAF(s,t) = 120000;
 387   
 388  C_elec(t)    = 0.010;
 389  C_ets(t)        = p0 + 17*(ord(t)-1);
 390  FIXEDCOST(t) = 000;
 391   
 392  C_quartz(t) = 50;
 393  C_Si(t)     = 250;
 394   
 395  C_fuel(fuel_all,t) = 0;
 396  C_fuel('coke',t)      = 300;
 397  C_fuel('oil',t)       = 500;
 398  C_fuel('biooil',t)    = 450;
 399  C_fuel('woodchips',t) = 120;
 400  C_fuel('COgas',t)     = 500;
 401  C_fuel('hydrogen',t)  = 2000;
 402  C_fuel('natgas',t)    = 150;
 403  C_fuel('biogas',t)    = 130;
 404  C_fuel('biochar',t)   = 400;
 405   
 406  phi_saf_lt(s,t,fuel_lt) = 0;
 407  phi_saf_ht(s,t,fuel_ht) = 0;
 408   
 409  Scalar
 410      LHV_coke       /  8000 /
 411      LHV_biochar    /  7000 /
 412      LHV_oil        / 11600 /
 413      LHV_biooil     /  5000 /
 414      LHV_woodchips  /  2500 /
