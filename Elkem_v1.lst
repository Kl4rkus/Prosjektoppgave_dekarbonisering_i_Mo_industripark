 432  * =========================
 433  * Solve
 434  * =========================
 435  Solve Elkem_FeSi_Opt using LP minimizing TotalCost;
 436   
 437  * ===== Per-period cost breakdown =====
 438  Set costcat / fixed, elec, fuel_SAF,
 439                quartz, silicon, ccs_var_ts, ccs_steam, ets,
 440                capex_ccs, capex_biocarb, total /;
 441   
 442  Parameter
 443      CostByT(t,costcat) "Cost breakdown by period (EUR)"
 444      TotalCost_t(t)     "Total cost per period (EUR)"
 445      TotalCost_check    "Sum over periods (EUR)";
 446   
 447  CostByT(t,'fixed')     = FIXEDCOST(t);
 448   
 449  CostByT(t,'elec') = C_elec(t) * ( sum(s, eSAF.l(s,t)) + eCCS_el.l(t) );
 450   
 451  CostByT(t,'fuel_SAF')  = sum(s, sum(fuel_all, C_fuel(fuel_all,t) * uSAF.l(s,t,fuel_all)));
 452   
 453  CostByT(t,'quartz')  = sum(s, C_quartz(t) * bQuartz.l(s,t));
 454  CostByT(t,'silicon') = sum(s, C_Si(t)     * bSi.l(s,t));
 455   
 456  CostByT(t,'ccs_var_ts') = ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured.l(t);
 457  CostByT(t,'ccs_steam')  = C_CCS_steam(t) * qCCS_th.l(t);
 458   
 459  CostByT(t,'ets') = C_ets(t) * CO2_ets.l(t);
 460   
 461  CostByT(t,'capex_ccs')     = 0;
 462  CostByT(t,'capex_ccs')$y(t)     = C_CCS_capex(t)       * qCCS.l(t);
 463  CostByT(t,'capex_biocarb') = 0;
 464  CostByT(t,'capex_biocarb')$y(t) = C_Biocarbon_capex(t) * qBiocarbon.l(t);
 465   
 466  CostByT(t,'total') =  CostByT(t,'fixed')
 467                      + CostByT(t,'elec')
 468                      + CostByT(t,'fuel_SAF')
 469                      + CostByT(t,'quartz')
 470                      + CostByT(t,'silicon')
 471                      + CostByT(t,'ccs_var_ts')
 472                      + CostByT(t,'ccs_steam')
 473                      + CostByT(t,'ets')
 474                      + CostByT(t,'capex_ccs')
 475                      + CostByT(t,'capex_biocarb');
 476   
 477  TotalCost_t(t)  = CostByT(t,'total');
 478  TotalCost_check = sum(t, TotalCost_t(t));
 479   
 480  * Fuel use per period & fuel (t)
 481  Parameter FuelUse_t_f(t,fuel_all) "Fuel use per period and fuel (t)";
 482  FuelUse_t_f(t,fuel_all) = sum(s, uSAF.l(s,t,fuel_all));
 483   
 484  execute_unload 'output_Elkem_FeSi.gdx'
 485      CO2_offgas_pot.l, CO2_atm_offgas.l,
 486      pFeSi.l, pFeSi_Si.l, pFeSi_SiO2.l,
 487      eSAF.l,
 488      bQuartz.l, bSi.l,
 489      uSAF.l,
 490      CO2_captured.l, CO2_total_net.l, qCCS.l, eCCS_el.l, qCCS_th.l,
 491      qBiocarbon.l, biocap.l,
 492      CostByT, TotalCost_t, TotalCost_check, costcat,
 493      FuelUse_t_f;
 494   
 495  option decimals=6;
 496   
 497  Parameter CO2_total(t) "Total CO2 per period (t, net)";
 498  CO2_total(t) = CO2_total_net.l(t);
 499  Scalar CO2_total_all "Total CO2 over all periods (t, net)";
 500  CO2_total_all = sum(t, CO2_total(t));
 501  Display CO2_total, CO2_total_all;
 502   
 503  Parameter
 504      ProdTot(t)         "Total FeSi (t)"
 505      ETS_cost(t)        "ETS cost per period (eur)"
 506      CO2_intensity(t)   "CO2 intensity vs total product (t CO2/t FeSi)";
 507   
 508  ProdTot(t)      = sum(s, pFeSi.l(s,t));
