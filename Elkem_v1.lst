 420  * Example mapping of fuels to HT/LT heat
 421  phi_saf_ht(s,t,'coke')     = LHV_coke;
 422  phi_saf_ht(s,t,'biochar')  = LHV_biochar;
 423  phi_saf_ht(s,t,'COgas')    = LHV_COgas;
 424  phi_saf_ht(s,t,'hydrogen') = LHV_hydrogen;
 425  phi_saf_ht(s,t,'natgas')   = LHV_natgas;
 426  phi_saf_ht(s,t,'biogas')   = LHV_biogas;
