 222  * =========================
 223  * Equations (append/replace CCS equations only)
 224  * =========================
 225   
 226  Equation
 227      co2_offgas_pot_def(t)   "CO2_t^{off-gas} = (sum gOffgas - D_offgas)*EF_flare, potential before CCS"
 228      ccs_stream_upper(t)     "f_t^{CCS} ≤ CO2_t^{off-gas}"
 229      ccs_capacity_upper(t)   "f_t^{CCS} ≤ sum_{y<=t} q_y^{CCS}"
 230      ccs_capture_eff(t)      "s_t^{CO2} ≤ ω · f_t^{CCS}"
 231      co2_atm_offgas_def(t)   "CO2^atm_offgas = CO2^off-gas - captured"
 232      co2_total_net_def(t)    "Net = sinter process CO2 + residual off-gas CO2"
 233      ccs_el_def(t)           "CCS electricity"
 234      ccs_th_def(t)           "CCS steam"
 235      invest_only_y(t)        "Forbid CCS investment outside y(t)"
 236      ;
 237   
 238  * Off-gas CO2 that could be emitted absent CCS (matches LaTeX)
 239  co2_offgas_pot_def(t)..
 240      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 241   
 242  * f_t^{CCS} ≤ CO2_t^{off-gas}
 243  ccs_stream_upper(t)..
 244      fCCS(t) =l= CO2_offgas_pot(t);
 245   
 246  * f_t^{CCS} ≤ K_t^{CCS} = sum_{y<=t} q_y^{CCS}
 247  ccs_capacity_upper(t)..
 248      fCCS(t) =l= sum(ty$( y(ty) and ord(ty) <= ord(t) ), qCCS(ty));
 249   
 250  * s_t^{CO2} ≤ ω · f_t^{CCS}
 251  ccs_capture_eff(t)..
 252      CO2_captured(t) =l= omega_ccs * fCCS(t);
 253   
 254  * CO2^atm_offgas = CO2^off-gas - s_t^{CO2}
 255  co2_atm_offgas_def(t)..
 256      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 257   
 258  * Net emissions include sinter process CO2 + residual off-gas
 259  co2_total_net_def(t)..
 260      CO2_total_net(t) =e= CO2_coke_sint(t) + CO2_atm_offgas(t);
 261   
 262  * CCS energy links
 263  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 264  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 265   
 266  * Forbid investment outside y
 267  invest_only_y(t)$(not y(t))..
 268      qCCS(t) =e= 0;
 269   
 270  * =========================
 271  * Constraints
 272  * =========================
 273  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 274  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 275   
 276  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 277   
 278  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 279  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 280  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 281   
 282  sinter_use_balance(t)..
 283      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 284   
 285  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 286  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 287   
 288  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 289  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 290   
 291  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 292   
 293  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 294  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 295   
 296  lt_saf_femn_def(s,t)..
 297      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 298  lt_saf_femn_min(s,t,fuel_lt)..
 299      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 300   
 301  ht_saf_femn_def(s,t)..
 302      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 303  ht_saf_femn_min(s,t,fuel_ht)..
 304      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 305   
 306  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 307  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 308   
 309  SiMn_material_balance(t)..
 310      sum(s, uSinterSiMn(s,t)) + pSlagTot(t) =e= sum(s, pSiMn(s,t));
 311   
 312  lt_saf_simn_def(s,t)..
 313      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 314  ht_saf_simn_def(s,t)..
 315      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 316   
 317  SiMn_energy_total(s,t)..
 318      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t) =e= TCF_SiMn(s,t) * pSiMn(s,t);
 319  SiMn_ht_min(s,t).. A_SAF_SiMn_HT_req(s) * pSiMn(s,t) =l= HT_SAF_SiMn(s,t);
 320   
 321  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t) + nu_SAF_SiMn(s,t) * pSiMn(s,t);
 322   
 323  * Off-gas production (FeMn + SiMn paths)
 324  offgas_def(s,t)..
 325      gOffgas(s,t) =e=
 326          sum(fuel_all,
 327                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 328              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 329          );
 330   
 331  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 332   
 333  * Legacy flaring CO2 (still reported)
 334  CO2_offgas_def(t)..
 335      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 336   
 337  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 338   
 339  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 340  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 341   
 342  obj..
 343      TotalCost =e=
 344        sum(t,
 345             FIXEDCOST(t)
 346           + C_ore(t)        * uOre(t)
 347           + C_buysinter(t)  * bSinter(t)
 348           + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) + eCCS_el(t) )
 349           + C_ets(t)        * CO2_total_net(t)
 350           + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 351           + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 352           + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 353           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 354           + C_CCS_steam(t) * qCCS_th(t)
 355          )
 356        + sum(t$y(t), C_CCS_capex(t) * qCCS(t));
