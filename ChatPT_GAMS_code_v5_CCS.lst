 221  * =========================
 222  * Equations (append/replace CCS equations only)
 223  * =========================
 224   
 225  Equation
 226      co2_offgas_pot_def(t)   "CO2_t^{off-gas} = (sum gOffgas - D_offgas)*EF_flare, potential before CCS"
 227      ccs_stream_upper(t)     "f_t^{CCS} ≤ CO2_t^{off-gas}"
 228      ccs_capacity_upper(t)   "f_t^{CCS} ≤ sum_{y<=t} q_y^{CCS}"
 229      ccs_capture_eff(t)      "s_t^{CO2} ≤ ω · f_t^{CCS}"
 230      co2_atm_offgas_def(t)   "CO2^atm_offgas = CO2^off-gas - captured"
 231      co2_total_net_def(t)    "Net = sinter process CO2 + residual off-gas CO2"
 232      ccs_el_def(t)           "CCS electricity"
 233      ccs_th_def(t)           "CCS steam"
 234      invest_only_y(t)        "Forbid CCS investment outside y(t)"
 235      ;
 236   
 237  * Off-gas CO2 that could be emitted absent CCS (matches LaTeX)
 238  co2_offgas_pot_def(t)..
 239      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 240   
 241  * f_t^{CCS} ≤ CO2_t^{off-gas}
 242  ccs_stream_upper(t)..
 243      fCCS(t) =l= CO2_offgas_pot(t);
 244   
 245  * f_t^{CCS} ≤ K_t^{CCS} = sum_{y<=t} q_y^{CCS}
 246  ccs_capacity_upper(t)..
 247      fCCS(t) =l= sum(ty$( y(ty) and ord(ty) <= ord(t) ), qCCS(ty));
 248   
 249  * s_t^{CO2} ≤ ω · f_t^{CCS}
 250  ccs_capture_eff(t)..
 251      CO2_captured(t) =l= omega_ccs * fCCS(t);
 252   
 253  * CO2^atm_offgas = CO2^off-gas - s_t^{CO2}
 254  co2_atm_offgas_def(t)..
 255      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 256   
 257  * Net emissions include sinter process CO2 + residual off-gas
 258  co2_total_net_def(t)..
 259      CO2_total_net(t) =e= CO2_coke_sint(t) + CO2_atm_offgas(t);
 260   
 261  * CCS energy links
 262  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 263  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 264   
 265  * Forbid investment outside y
 266  invest_only_y(t)$(not y(t))..
 267      qCCS(t) =e= 0;
 268   
 269  * =========================
 270  * Constraints
 271  * =========================
 272  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 273  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 274   
 275  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 276   
 277  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 278  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 279  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 280   
 281  sinter_use_balance(t)..
 282      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 283   
 284  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 285  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 286   
 287  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 288  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 289   
 290  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 291   
 292  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 293  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 294   
 295  lt_saf_femn_def(s,t)..
 296      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 297  lt_saf_femn_min(s,t,fuel_lt)..
 298      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 299   
 300  ht_saf_femn_def(s,t)..
 301      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 302  ht_saf_femn_min(s,t,fuel_ht)..
 303      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 304   
 305  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 306  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 307   
 308  SiMn_material_balance(t)..
 309      sum(s, uSinterSiMn(s,t)) + pSlagTot(t) =e= sum(s, pSiMn(s,t));
 310   
 311  lt_saf_simn_def(s,t)..
 312      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 313  ht_saf_simn_def(s,t)..
 314      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 315   
 316  SiMn_energy_total(s,t)..
 317      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t) =e= TCF_SiMn(s,t) * pSiMn(s,t);
 318  SiMn_ht_min(s,t).. A_SAF_SiMn_HT_req(s) * pSiMn(s,t) =l= HT_SAF_SiMn(s,t);
 319   
 320  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t) + nu_SAF_SiMn(s,t) * pSiMn(s,t);
 321   
 322  * Off-gas production (FeMn + SiMn paths)
 323  offgas_def(s,t)..
 324      gOffgas(s,t) =e=
 325          sum(fuel_all,
 326                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 327              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 328          );
 329   
 330  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 331   
 332  * Legacy flaring CO2 (still reported)
 333  CO2_offgas_def(t)..
 334      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 335   
 336  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 337   
 338  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 339  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 340   
 341  obj..
 342      TotalCost =e=
 343        sum(t,
 344             FIXEDCOST(t)
 345           + C_ore(t)        * uOre(t)
 346           + C_buysinter(t)  * bSinter(t)
 347           + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) + eCCS_el(t) )
 348           + C_ets(t)        * CO2_total_net(t)
 349           + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 350           + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 351           + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 352           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 353           + C_CCS_steam(t) * qCCS_th(t)
 354          )
 355        + sum(t$y(t), C_CCS_capex(t) * qCCS(t));
