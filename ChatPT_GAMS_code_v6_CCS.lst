 224  *-----------------------------*
 225  * Constraints                 *
 226  *-----------------------------*
 227   
 228  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 229  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 230   
 231  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 232   
 233  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 234  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 235  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 236   
 237  sinter_use_balance(t)..
 238      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 239   
 240  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 241  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 242   
 243  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 244  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 245   
 246  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 247   
 248  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 249  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 250   
 251  lt_saf_femn_def(s,t)..
 252      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 253  lt_saf_femn_min(s,t,fuel_lt)..
 254      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 255   
 256  ht_saf_femn_def(s,t)..
 257      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 258  ht_saf_femn_min(s,t,fuel_ht)..
 259      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 260   
 261  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 262  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 263   
 264  SiMn_material_balance(t)..
 265      sum(s, uSinterSiMn(s,t)) + pSlagTot(t) =e= sum(s, pSiMn(s,t));
 266   
 267  lt_saf_simn_def(s,t)..
 268      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 269  ht_saf_simn_def(s,t)..
 270      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 271   
 272  SiMn_energy_total(s,t)..
 273      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t) =e= TCF_SiMn(s,t) * pSiMn(s,t);
 274  SiMn_ht_min(s,t).. A_SAF_SiMn_HT_req(s) * pSiMn(s,t) =l= HT_SAF_SiMn(s,t);
 275   
 276  e_saf_def(s,t).. eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t) + nu_SAF_SiMn(s,t) * pSiMn(s,t);
 277   
 278  * --- Off-gas production (FeMn + SiMn paths) ---
 279  offgas_def(s,t)..
 280      gOffgas(s,t) =e=
 281          sum(fuel_all,
 282                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 283              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 284          );
 285   
 286  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 287   
 288  * --- Flared off-gas bounds (non-negative) ---
 289  flare_lower(t).. Fflare(t) =g= sum(s, gOffgas(s,t)) - D_offgas(t);
 290  flare_upper(t).. Fflare(t) =l= sum(s, gOffgas(s,t));
 291   
 292  * --- Net CO2 from flaring AFTER CCS capture credit ---
 293  CO2_offgas_def(t)..
 294      CO2_offgas(t) =e= EF_flare * Fflare(t) - sCCS(t);
 295   
 296  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 297   
 298  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 299  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 300   
 301  * -------- CCS: investment mask, stock, capacity/use, physics --------
 302  invest_mask(t)..  qCCS(t) =e= investAllowed(t) * qCCS(t);
 303  ccs_stock(t)..    KCCS(t) =e= sum(tt, cumMap(t,tt) * qCCS(tt));
 304  ccs_cap_bound(t)..sCCS(t) =l= KCCS(t);
 305  ccs_phys_bound(t)..sCCS(t) =l= omega * EF_flare * Fflare(t);
 306   
 307  * Objective (adds CAPEX, fixed and variable CCS costs)
 308  obj..
 309      TotalCost =e= sum(t,
 310                   FIXEDCOST(t)
 311                 + C_ore(t)        * uOre(t)
 312                 + C_buysinter(t)  * bSinter(t)
 313                 + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) )
 314                 + C_ets(t)        * ( CO2_coke_sint(t) + CO2_offgas(t) )
 315                 + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 316                 + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 317                 + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 318                 + C_FIX_CCS(t)    * KCCS(t)
 319                 + C_VAR_CCS(t)    * sCCS(t)
 320                 )
 321                 + sum(t, C_CAPEX_CCS(t) * qCCS(t));
 322   
 323  Model FeMn_SiMn_Opt / all /;
