 213      sinter_use_balance(t)
 214      lt_sint_def(t)
 215      lt_sint_min_lt(fuel_lt,t)
 216      ht_sint_def(t)
 217      ht_sint_min_ht(fuel_ht,t)
 218   
 219      FeMn_sinter_link(s,t)
 220      FeMn_energy_total(s,t)
 221      FeMn_ht_min(s,t)
 222      lt_saf_femn_def(s,t)
 223      lt_saf_femn_min(s,t,fuel_lt)
 224      ht_saf_femn_def(s,t)
 225      ht_saf_femn_min(s,t,fuel_ht)
 226   
 227      slag_def(s,t)
 228      slag_tot_def(t)
 229      SiMn_material_balance(t)
 230      pSiMn_split_def(s,t)
 231      uSinterSiMn_split_def(s,t)
 232      quartz_balance(s,t)
 233      si_balance(s,t)
 234   
 235      lt_saf_simn_def(s,t)
 236      ht_saf_simn_def(s,t)
 237      SiMn_energy_total(s,t)
 238      SiMn_ht_min(s,t)
