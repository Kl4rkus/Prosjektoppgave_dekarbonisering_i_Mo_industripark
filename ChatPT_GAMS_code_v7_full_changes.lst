 351  * =========================
 352  * Constraints
 353  * =========================
 354   
 355  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 356  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 357   
 358  * Split total SiMn into Si and SiO2 routes
 359  pSiMn_split_def(s,t).. pSiMn(s,t) =e= pSiMn_Si(s,t) + pSiMn_SiO2(s,t);
 360  uSinterSiMn_split_def(s,t)..
 361      uSinterSiMn(s,t) =e= uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t);
 362   
 363  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 364   
 365  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 366  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 367  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 368   
 369  sinter_use_balance(t)..
 370      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 371   
 372  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 373  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 374   
 375  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 376  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 377   
 378  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 379   
 380  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 381  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 382   
 383  lt_saf_femn_def(s,t)..
 384      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 385  lt_saf_femn_min(s,t,fuel_lt)..
 386      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 387   
 388  ht_saf_femn_def(s,t)..
 389      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 390  ht_saf_femn_min(s,t,fuel_ht)..
 391      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 392   
 393  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 394  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 395   
 396  * SiMn material balance including slag & sinter
 397  SiMn_material_balance(t)..
 398      Y_sinter_SiMn * sum(s, uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t))
 399    + Y_slag_SiMn   * pSlagTot(t)
 400      =e= sum(s, pSiMn_Si(s,t) + pSiMn_SiO2(s,t));
 401   
 402  * Quartz and Si balances
 403  quartz_balance(s,t).. bQuartz(s,t) =e= Y_SiMn_SiO2_quartz * pSiMn_SiO2(s,t);
 404  si_balance(s,t)..     bSi(s,t)     =e= Y_SiMn_Si_silicon  * pSiMn_Si(s,t);
 405   
 406  lt_saf_simn_def(s,t)..
 407      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 408  ht_saf_simn_def(s,t)..
 409      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 410   
 411  * Route-specific SiMn heat
 412  SiMn_energy_total(s,t)..
 413      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t)
 414      =e= TCF_SiMn_Si(s,t)   * pSiMn_Si(s,t)
 415         + TCF_SiMn_SiO2(s,t)* pSiMn_SiO2(s,t);
 416   
 417  SiMn_ht_min(s,t)..
 418      A_SAF_SiMn_HT_req(s) * (pSiMn_Si(s,t) + pSiMn_SiO2(s,t)) =l= HT_SAF_SiMn(s,t);
 419   
 420  * Route-specific electricity
 421  e_saf_def(s,t)..
 422      eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t)
 423                + nu_SAF_Si(s,t)      * pSiMn_Si(s,t)
 424                + nu_SAF_SiO2(s,t)    * pSiMn_SiO2(s,t);
 425   
 426  * Off-gas production (FeMn + SiMn paths)
 427  offgas_def(s,t)..
 428      gOffgas(s,t) =e=
 429          sum(fuel_all,
 430                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 431              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 432          );
 433   
 434  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 435   
 436  CO2_offgas_def(t)..
 437      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 438   
 439  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 440   
 441  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 442  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 443   
 444  * Biocarbon capacity accumulation and usage limit
 445  biocap_def(t)..
 446      biocap(t) =e=
 447         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_Biocarbon ),
 448             qBiocarbon(ty));
 449   
 450  biocarbon_use_limit(t)..
 451      sum(s, uSAF(s,t,'biochar') + uSAF_SiMn(s,t,'biochar'))
 452    + uSint_HT(t,'biochar')
 453      =l= biocap(t);
 454   
 455  * =========================
 456  * CCS equations
 457  * =========================
 458   
 459  co2_offgas_pot_def(t)..
 460      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 461   
