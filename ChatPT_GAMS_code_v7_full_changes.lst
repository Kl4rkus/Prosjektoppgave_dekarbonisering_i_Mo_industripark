 266  * =========================
 267  * Constraints
 268  * =========================
 269   
 270  dem_FeMn(t).. sum(s, pFeMn(s,t)) =g= D_FeMn(t);
 271  dem_SiMn(t).. sum(s, pSiMn(s,t)) =g= D_SiMn(t);
 272   
 273  * Split total SiMn into Si and SiO2 routes
 274  pSiMn_split_def(s,t).. pSiMn(s,t) =e= pSiMn_Si(s,t) + pSiMn_SiO2(s,t);
 275  uSinterSiMn_split_def(s,t)..
 276      uSinterSiMn(s,t) =e= uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t);
 277   
 278  ore_to_sinter(t).. uOre(t) =e= Y_ore_to_sinter * pSinter(t);
 279   
 280  sinter_energy_total(t).. LT_sint(t) + HT_sint(t) =e= TCF_sint(t) * pSinter(t);
 281  sinter_ht_min(t)..       A_sint_HT_req * pSinter(t) =l= HT_sint(t);
 282  sinter_power(t)..        eSint(t) =e= eta_sint * pSinter(t);
 283   
 284  sinter_use_balance(t)..
 285      sum(s, uSinterFeMn(s,t)) + sum(s, uSinterSiMn(s,t)) =e= pSinter(t) + bSinter(t);
 286   
 287  lt_sint_def(t).. LT_sint(t) =e= sum(fuel_lt, phi_sint_lt(t,fuel_lt) * uSint_LT(t,fuel_lt));
 288  lt_sint_min_lt(fuel_lt,t).. A_sint_lt(fuel_lt) * LT_sint(t) =l= uSint_LT(t,fuel_lt);
 289   
 290  ht_sint_def(t).. HT_sint(t) =e= sum(fuel_ht, phi_sint_ht(t,fuel_ht) * uSint_HT(t,fuel_ht));
 291  ht_sint_min_ht(fuel_ht,t).. A_sint_ht(fuel_ht) * HT_sint(t) =l= uSint_HT(t,fuel_ht);
 292   
 293  FeMn_sinter_link(s,t).. uSinterFeMn(s,t) =e= Y_sinter_to_FeMn * pFeMn(s,t);
 294   
 295  FeMn_energy_total(s,t).. LT_SAF_FeMn(s,t) + HT_SAF_FeMn(s,t) =e= TCF_FeMn(s,t) * pFeMn(s,t);
 296  FeMn_ht_min(s,t)..       A_SAF_HT_req(s) * pFeMn(s,t) =l= HT_SAF_FeMn(s,t);
 297   
 298  lt_saf_femn_def(s,t)..
 299      LT_SAF_FeMn(s,t) =e= sum(fuel_lt, phi_saf_lt(s,t,fuel_lt) * uSAF(s,t,fuel_lt));
 300  lt_saf_femn_min(s,t,fuel_lt)..
 301      A_saf_lt(s,fuel_lt) * LT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_lt);
 302   
 303  ht_saf_femn_def(s,t)..
 304      HT_SAF_FeMn(s,t) =e= sum(fuel_ht, phi_saf_ht(s,t,fuel_ht) * uSAF(s,t,fuel_ht));
 305  ht_saf_femn_min(s,t,fuel_ht)..
 306      A_saf_ht(s,fuel_ht) * HT_SAF_FeMn(s,t) =l= uSAF(s,t,fuel_ht);
 307   
 308  slag_def(s,t)..   pSlag(s,t) =e= Y_sinter_to_slag * uSinterFeMn(s,t);
 309  slag_tot_def(t).. pSlagTot(t) =e= sum(s, pSlag(s,t));
 310   
 311  * SiMn material balance including slag & sinter
 312  SiMn_material_balance(t)..
 313      Y_sinter_SiMn * sum(s, uSinterSiMn_Si(s,t) + uSinterSiMn_SiO2(s,t))
 314    + Y_slag_SiMn   * pSlagTot(t)
 315      =e= sum(s, pSiMn_Si(s,t) + pSiMn_SiO2(s,t));
 316   
 317  * Quartz and Si balances
 318  quartz_balance(s,t).. bQuartz(s,t) =e= Y_SiMn_SiO2_quartz * pSiMn_SiO2(s,t);
 319  si_balance(s,t)..     bSi(s,t)     =e= Y_SiMn_Si_silicon  * pSiMn_Si(s,t);
 320   
 321  lt_saf_simn_def(s,t)..
 322      LT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_lt(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 323  ht_saf_simn_def(s,t)..
 324      HT_SAF_SiMn(s,t) =e= sum(fuel_all, phi_saf_smn_ht(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all));
 325   
 326  * Route-specific SiMn heat
 327  SiMn_energy_total(s,t)..
 328      LT_SAF_SiMn(s,t) + HT_SAF_SiMn(s,t)
 329      =e= TCF_SiMn_Si(s,t)   * pSiMn_Si(s,t)
 330         + TCF_SiMn_SiO2(s,t)* pSiMn_SiO2(s,t);
 331   
 332  SiMn_ht_min(s,t)..
 333      A_SAF_SiMn_HT_req(s) * (pSiMn_Si(s,t) + pSiMn_SiO2(s,t)) =l= HT_SAF_SiMn(s,t);
 334   
 335  * Route-specific electricity
 336  e_saf_def(s,t)..
 337      eSAF(s,t) =e= eta_SAF_FeMn(s,t) * pFeMn(s,t)
 338                + nu_SAF_Si(s,t)      * pSiMn_Si(s,t)
 339                + nu_SAF_SiO2(s,t)    * pSiMn_SiO2(s,t);
 340   
 341  * Off-gas production (FeMn + SiMn paths)
 342  offgas_def(s,t)..
 343      gOffgas(s,t) =e=
 344          sum(fuel_all,
 345                Y_offgas(s,t,fuel_all)     * uSAF(s,t,fuel_all)
 346              + Y_offgas_smn(s,t,fuel_all) * uSAF_SiMn(s,t,fuel_all)
 347          );
 348   
 349  CO2_coke_sint_def(t).. CO2_coke_sint(t) =e= EF_coke_sint * pSinter(t);
 350   
 351  CO2_offgas_def(t)..
 352      CO2_offgas(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 353   
 354  coke_total_def(t).. uTotalCoke_SAF(t) =e= sum(s, uSAF(s,t,'coke'));
 355   
 356  capacity_saf(s,t)..  pFeMn(s,t) + pSiMn(s,t) =l= Q_SAF(s,t);
 357  capacity_sinter(t).. pSinter(t)              =l= Q_sint(t);
 358   
 359  * Biocarbon capacity accumulation and usage limit
 360  biocap_def(t)..
 361      biocap(t) =e=
 362         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_Biocarbon ),
 363             qBiocarbon(ty));
 364   
 365  biocarbon_use_limit(t)..
 366      sum(s, uSAF(s,t,'biochar') + uSAF_SiMn(s,t,'biochar'))
 367    + uSint_HT(t,'biochar')
 368      =l= biocap(t);
 369   
 370  * =========================
 371  * CCS equations
 372  * =========================
 373   
 374  co2_offgas_pot_def(t)..
 375      CO2_offgas_pot(t) =e= ( sum(s, gOffgas(s,t)) - D_offgas(t) ) * EF_flare;
 376   
 377  ccs_stream_upper(t)..      fCCS(t) =l= CO2_offgas_pot(t);
 378   
 379  * Lifetime-limited CCS capacity
 380  ccs_capacity_upper(t)..
 381      fCCS(t) =l=
 382         sum(ty$( y(ty) and ord(t) >= ord(ty) and (ord(t) - ord(ty)) < L_CCS ),
 383             qCCS(ty));
 384   
 385  ccs_capture_eff(t)..
 386      CO2_captured(t) =l= omega_ccs * fCCS(t);
 387   
 388  co2_atm_offgas_def(t)..
 389      CO2_atm_offgas(t) =e= CO2_offgas_pot(t) - CO2_captured(t);
 390   
 391  co2_total_net_def(t)..
 392      CO2_total_net(t) =e= CO2_coke_sint(t) + CO2_atm_offgas(t);
 393   
 394  ccs_el_def(t)..  eCCS_el(t) =e= SEC_CCS_el(t) * CO2_captured(t);
 395  ccs_th_def(t)..  qCCS_th(t) =e= SEC_CCS_th(t) * CO2_captured(t);
 396   
 397  invest_only_y(t)$(not y(t))..
 398      qCCS(t) =e= 0;
 399   
 400  invest_biocarbon_only_y(t)$(not y(t))..
 401      qBiocarbon(t) =e= 0;
 402   
 403  * =========================
 404  * Objective function
 405  * =========================
 406  obj..
 407      TotalCost =e=
 408        sum(t,
 409             FIXEDCOST(t)
 410           + C_ore(t)        * uOre(t)
 411           + C_buysinter(t)  * bSinter(t)
 412           + C_elec(t)       * ( sum(s, eSAF(s,t)) + eSint(t) + eCCS_el(t) )
 413           + C_ets(t)        * CO2_total_net(t)
 414           + sum(s, sum(fuel_all, C_fuel(fuel_all,t) * ( uSAF(s,t,fuel_all) + uSAF_SiMn(s,t,fuel_all) )))
 415           + sum(fuel_lt, C_fuel(fuel_lt,t) * uSint_LT(t,fuel_lt))
 416           + sum(fuel_ht, C_fuel(fuel_ht,t) * uSint_HT(t,fuel_ht))
 417           + ( C_CCS_var(t) + C_CCS_TandS(t) ) * CO2_captured(t)
 418           + C_CCS_steam(t) * qCCS_th(t)
 419           + sum(s, C_quartz(t) * bQuartz(s,t))
 420           + sum(s, C_Si(t)     * bSi(s,t))
 421          )
 422        + sum(t$y(t), C_CCS_capex(t)        * qCCS(t))
 423        + sum(t$y(t), C_Biocarbon_capex(t)  * qBiocarbon(t));
 424   
 425  * =========================
 426  * Model & Test data
 427  * =========================
 428  Model FeMn_SiMn_Opt / all /;
 429   
 430  * --- Test data (example numbers) ---
 431   
 432  D_FeMn(t)  = 1000;
 433  D_SiMn(t)  = 1000;
