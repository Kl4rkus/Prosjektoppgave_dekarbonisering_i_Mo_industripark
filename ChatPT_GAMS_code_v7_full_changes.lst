 253      co2_offgas_pot_def(t)
 254      ccs_stream_upper(t)
 255      ccs_capacity_upper(t)
 256      ccs_capture_eff(t)
 257      co2_atm_offgas_def(t)
 258      co2_total_net_def(t)
