 240      e_saf_def(s,t)
 241   
 242      offgas_def(s,t)
 243      CO2_coke_sint_def(t)
 244      CO2_offgas_def(t)
 245      coke_total_def(t)
 246   
 247      capacity_saf(s,t)
 248      capacity_sinter(t)
